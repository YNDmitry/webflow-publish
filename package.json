{
  "name": "webflow-publish",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently --raw -n \"vite\" \"wf-publish --env=dev --dev-port=5173\"",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "publish:global": "wf-publish --env=prod",
    "publish:pages": "wf-publish --env=prod --by-page",
    "lint": "eslint src/**/**/*.{js,ts}",
    "format": "prettier --write src/**/**/*.{js,ts}",
    "lint:fix": "eslint src/scripts/**/**/*.{js,ts} --fix",
    "prepare": "npm run lint && npm run format"
  },
  "devDependencies": {
    "typescript": "~5.6.2",
    "vite": "^6.0.5"
  },
  "dependencies": {
    "@types/jquery": "^3.5.32",
    "@typescript-eslint/eslint-plugin": "^8.19.0",
    "@typescript-eslint/parser": "^8.19.0",
    "@yndmitry/vite-plugin-pages": "^0.1.4",
    "concurrently": "^9.1.2",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "prettier": "^3.4.2"
  },
  "config": {
    "ghUserName": "yndmitry",
    "repositoryName": "webflow-publish",
    "browser": "arc",
    "packageManager": "bun"
  }
}
